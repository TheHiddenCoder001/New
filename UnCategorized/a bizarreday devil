	Action = "Return",
	Request = "IsPreloaded",
	AssetID = 0 -- Audio ID
})
-- Playing
ABizarreSoundSystem:Perform({
	Action = "Return",
	Request = "IsPlaying",
	AssetID = 0 -- Audio ID
})
-- Length
ABizarreSoundSystem:Perform({
	Action = "Return",
	Request = "AudioLength",
	AssetID = 0 -- Audio ID
})
