function spikes()
    if HalfDemon == true then return end
    local knife = Instance.new("Part")
    knife.Anchored = false
    knife.CanCollide = false
    knife.Massless = true
    knife.Transparency = 1
    knife.Size = Vector3.new(10,10,10)
    knife.CFrame = hrp.CFrame * CFrame.new(0,0,0)
    knife.Parent = Model

    local boopyve = Instance.new("BodyVelocity")
    boopyve.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    boopyve.P = math.huge
    boopyve.Parent = knife
    boopyve.Name = "Force"

    coroutine.resume(coroutine.create(function()
        local ProjectileAuraSetter = 0
        repeat
        	travdis = travdis + 1
        	coroutine.resume(coroutine.create(function()
        --	rs.Damage:FireServer(human, knife.CFrame * CFrame.new(0,-6,1.75), 0, 0,Vector3.new(0, 0, 0), "rbxassetid://", 1, Color3.new(255, 255, 0), "", 1, 1)
         		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, knife.CFrame * CFrame.new(0,-5,1.75), 0, 0, nil, 0.1, "", 1, 0)

        	end))
        	boopyve.Velocity = knife.CFrame.lookVector * 100
        	coroutine.resume(coroutine.create(function()
        	for i,v in pairs(game.Workspace.Entities:GetChildren()) do
            	if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
                	if (knife.Position - v.HumanoidRootPart.Position).Magnitude < 80 then
                    --	knife.CFrame = CFrame.lookAt(knife.Position, v.HumanoidRootPart.Position)
                    	wait(9e999)
                	end
            	end
        	end
        	end))
            wait()
        until travdis == 50 or travdis > 50
        travdis = 0
        knife:Destroy()
    end))

    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = knife.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent ~= chr then
                if debounce == false then
                debounce = true
                delay(.01, function() debounce = false end)
            game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
            game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(hit.Parent.Humanoid)
            game:GetService("ReplicatedStorage").Damage11:FireServer(human, hit.Parent.HumanoidRootPart.CFrame*CFrame.new(math.random(-5,5),math.random(-1,5),math.random(-5,5)), 0, 0, Vector3.new(0, 0, 0), .05, "", 1, 1)
             
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Parent ~= chr then
                if debounce == false then
                debounce = true
                delay(.01, function() debounce = false end)
            game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Parent.Humanoid)
            game.ReplicatedStorage.Knock:FireServer(hit.Parent.Parent.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(hit.Parent.Humanoid)
            game:GetService("ReplicatedStorage").Damage11:FireServer(human, hit.Parent.Parent.HumanoidRootPart.CFrame*CFrame.new(math.random(-5,5),math.random(-1,5),math.random(-5,5)), 0, 0, Vector3.new(0, 0, 0), .05, "", 1, 1)
             
            end
        end
    end)
             --HitBox(Player.Character["Torso"], 75.5, 0.2, 0.3, 99, Player.Character.Torso.CFrame.lookVector * 175 + Vector3.new(0, 45, 0), 0.25, "rbxassetid://3240049847", false, true, 10.35, true)
end
Mouse.KeyDown:Connect(function(Key)
    if Key == 'p' then
        if HalfDemon == false then
          spikes()
        elseif HalfDemon == true then
          spikes2()

end     
    end
end)
Mouse.KeyDown:Connect(function(Key)
    if Key == 'g' then
        if HalfDemon == true then
        spikesnoloopcam()

end     
    end
end)
 Mouse.KeyUp:Connect(function(Key)
    if Key == 'p' then
 
          SpikeHolder = false
     
    end
end)
 Mouse.KeyUp:Connect(function(Key)
    if Key == 'g' then
 
          SpikeHolder = false
     
    end
end)
local LeSun = false
function Sun()
    if LeSun == false then
    LeSun = true
     
        	coroutine.resume(coroutine.create(function()
    repeat wait(0.1)
         for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 150 then
            game:GetService("ReplicatedStorage").Knock:FireServer(v.Humanoid)
            game:GetService("ReplicatedStorage").BurnDamage:FireServer(v.Humanoid,v.HumanoidRootPart.CFrame,0,0,Vector3.new(0,0,0),"rbxassetid://241837157",0,Color3.new(255, 255, 255),"",0,0)
            end
        end
         end
		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-40,40), 0, 0, nil, 0.65, "", 1, 0)
				game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-40,-40), 0, 0, nil, 0.65, "", 1, 0)
				game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(40,-40,0), 0, 0, nil, 0.65, "", 1, 0)
								game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(-40,-40,0), 0, 0, nil, 0.65, "", 1, 0)



    Remotes.Damage:FireServer(Player.Character.Humanoid,Player.Character.Head.CFrame * CFrame.new(0, 50, 0),.2,0,Vector3.new(0,-40,0),"rbxassetid://0",100,Color3.new(1,0,0),"rbxassetid://1091083826",0,0)
    until LeSun == false
        	end))
elseif LeSun == true then
    LeSun = false
elseif HalfDemon == true and LeSun == false then
    LeSun = false
    end 
end

Mouse.KeyDown:Connect(function(Key)
    if Key == 'u' then
        if HalfDemon == true then return end
        Sun()
    end
end)
local Yes = 0
 
function ledemon()
 if HalfDemon == false then
    HalfDemon = true
     
        	coroutine.resume(coroutine.create(function()
    repeat wait()
  Yes = Yes + 1

game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Head"].CFrame * CFrame.new(math.sin(Yes * 50)*3, math.cos(Yes * 50)*3, 3), 0, 0, Vector3.new(nil, nil,nil), "rbxassetid://245751634", 3, Color3.new(0, 0, 0), "", 1, 1)
game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Head"].CFrame * CFrame.new(math.sin(Yes * 50)*-3, math.cos(Yes * 50)*3, 3), 0, 0, Vector3.new(nil, nil,nil), "rbxassetid://245751634", 3, Color3.new(1, 0, 0), "", 1, 1)
    until HalfDemon == false
        	end))
elseif HalfDemon == true then
    HalfDemon = false
    end 
end

Mouse.KeyDown:Connect(function(Key)
    if Key == "-" then
			ledemon()
			end
end)
