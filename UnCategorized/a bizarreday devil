local desti = false
local AuraSetterer = 0
local chr = game.Players.LocalPlayer.Character
local human = chr.Humanoid
function Super()
    local PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Punch)
	 PlayAudio(1371567007,5465465,1)
		PunchAnim:Play(0.1, 1, 1)
		PunchAnim:AdjustSpeed(1.2)
 
	 
		desti = true
		coroutine.resume(coroutine.create(function()
		repeat
            AuraSetterer = AuraSetterer + 1
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetterer * 50)*0, math.cos(AuraSetterer * 50)*1, math.sin(AuraSetterer * 50)*0), 0, 0, Vector3.new(0,0,0), "rbxassetid://245751634", 3, Color3.new(1,0,0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetterer * 50)*0, math.cos(AuraSetterer * 50)*2, math.sin(AuraSetterer * 50)*1), 0, 0, Vector3.new(0,0,0), "rbxassetid://", 3, Color3.new(1,0,0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetterer * 50)*0, math.cos(AuraSetterer * 50)*-1, math.sin(AuraSetterer * 50)*0), 0, 0, Vector3.new(0,0,0), "rbxassetid://245751634", 3,Color3.new(1,0,0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetterer * 50)*0, math.cos(AuraSetterer * 50)*-2, math.sin(AuraSetterer * 50)*-2), 0, 0, Vector3.new(0,0,0), "rbxassetid://", 3, Color3.new(1,0,0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetterer * 50)*0, math.cos(AuraSetterer * 50)*-2, math.sin(AuraSetterer* 50)*3), 0, 0, Vector3.new(0,0,0), "rbxassetid://245751634", 3, Color3.new(1,0,0), "", 1, 1)
            wait()
		until game.Players.LocalPlayer.Character.Humanoid.Health < 0.001 or desti == false
		end))
		wait(0.55)
		 PunchAnim:AdjustSpeed(2.5) 
        _G.supastronkpunch4mode()
		 PunchAnim:AdjustSpeed(0)
	 
	game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(.95, 1, .95), Vector3.new(.95, 1, .95), BrickColor.new("Bright red"))
game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(.95, .1, .95), Vector3.new(.95, .1, .95), BrickColor.new("Bright red"))
game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-.95, -1, -.95), Vector3.new(-.95, -1, -.95), BrickColor.new("Bright red"))
game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-1, -1.5, -1), Vector3.new(-1, .15, 1), BrickColor.new("Bright red"))

  
		wait(0.1)
		PunchAnim:AdjustSpeed(1)
		 desti = false
		
	 
	 
end
