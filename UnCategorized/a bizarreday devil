--[[ Usage:
-- Sound Creation
ABizarreSoundSystem:Perform({
	Action = "Create",
	AssetID = 0, -- Audio ID
	PlaybackSpeed = 1, -- Pitch
	Volume = 10,
	Loop = false, -- Loops the song
	StartPosition = 45, -- Starts the song at this time position, 0 or nil for no change
	KeepPositionOnLoop = false -- Starts the song at the given time position on loop, keep false to start over
})

-- Sound Preload (Automatically Preloads if Played without being Preloaded)
ABizarreSoundSystem:Perform({
	Action = "Preload",
	AssetID = 0 -- Audio ID
})

-- Sound Stopping
ABizarreSoundSystem:Perform({
	Action = "Destroy",
	AssetID = 0 -- Audio ID, set to "All" in order to stop all ongoing sounds
})

-- Sound Changing
ABizarreSoundSystem:Perform({
	Action = "Change",
	Change = "Volume", -- or "PlaybackSpeed"
	AssetID = 0, -- Audio ID, set to "All" in order to change all sounds
	Value = 1 -- Changed Value
})

-- Sound Checks
-- Preloaded
ABizarreSoundSystem:Perform({
	Action = "Return",
	Request = "IsPreloaded",
	AssetID = 0 -- Audio ID
})
-- Playing
ABizarreSoundSystem:Perform({
	Action = "Return",
	Request = "IsPlaying",
	AssetID = 0 -- Audio ID
})
-- Length
ABizarreSoundSystem:Perform({
	Action = "Return",
	Request = "AudioLength",
	AssetID = 0 -- Audio ID
})
]]

local LocalPlayer = game:GetService("Players").LocalPlayer 

local ABizarreSoundSystem = {
	Sounds = {},
	Preloaded = {},
	GlobalExistenceTime = 0.69625,
	AudioRemote = game:GetService("ReplicatedStorage").Damage12
}

function Thread(Function)
	coroutine.resume(coroutine.create(Function))
end

function ABizarreSoundSystem:Perform(Arguments)
	if Arguments.Action == "Preload" then
		local PreloadAudio = Instance.new("Sound")
		PreloadAudio.SoundId = "rbxassetid://"..Arguments.AssetID
		PreloadAudio.Parent = game:GetService("ReplicatedStorage")
		game:GetService("ContentProvider"):PreloadAsync({PreloadAudio})
		repeat game:GetService("RunService").RenderStepped:Wait() until PreloadAudio.TimeLength and PreloadAudio.TimeLength > 0
		table.insert(ABizarreSoundSystem.Preloaded, {Arguments.AssetID, PreloadAudio.TimeLength})
		game:GetService("Debris"):AddItem(PreloadAudio, 0.5)
	elseif Arguments.Action == "Return" then
		if Arguments.Request == "IsPreloaded" then
			for Index, Table in next, ABizarreSoundSystem.Preloaded do
				if Table[1] == Arguments.AssetID then
					return true
				end
			end
			return false
		elseif Arguments.Request == "AudioLength" then
			for Index, Table in next, ABizarreSoundSystem.Preloaded do
				if Table[1] == Arguments.AssetID then
					return Table[2]
				end
			end
			return nil
		elseif Arguments.Request == "IsPlaying" then
			for ID, System in next, ABizarreSoundSystem.Sounds do
				if System.AssetID == Arguments.AssetID then
					return true
				end
			end
			return false
		end
		return nil
	elseif Arguments.Action == "Destroy" then
		if Arguments.AssetID ~= "All" then
			for ID, System in next, ABizarreSoundSystem.Sounds do
				if System.AssetID == Arguments.AssetID then
					System.DoesLoop = false
					System:Perform({Action = "Break"})
					ABizarreSoundSystem.Sounds[ID] = nil
					System = nil
				end
			end
		else
			for ID, System in next, ABizarreSoundSystem.Sounds do
				System.DoesLoop = false
				System:Perform({Action = "Break"})
				ABizarreSoundSystem.Sounds[ID] = nil
				System = nil
			end
			ABizarreSoundSystem.Sounds = {}
		end
	elseif Arguments.Action == "Change" then
		if Arguments.AssetID ~= "All" then
			for ID, System in next, ABizarreSoundSystem.Sounds do
				if System.AssetID == Arguments.AssetID then
					System:Perform({Action = "Change", Change = Arguments.Change, Value = Arguments.Value})
				end
			end
		else
			for ID, System in next, ABizarreSoundSystem.Sounds do
				System:Perform({Action = "Change", Change = Arguments.Change, Value = Arguments.Value})
			end
		end
	elseif Arguments.Action == "Create" then
		if Arguments.DisableAutomaticPreload ~= true then
			if ABizarreSoundSystem:Perform({Action = "Return", Request = "IsPreloaded", AssetID = Arguments.AssetID}) == false then
				ABizarreSoundSystem:Perform({Action = "Preload", AssetID = Arguments.AssetID})
			end
		end
		if ABizarreSoundSystem:Perform({Action = "Return", Request = "IsPlaying", AssetID = Arguments.AssetID}) == true then
			ABizarreSoundSystem:Perform({Action = "Destroy", AssetID = Arguments.AssetID})
		end
		local SoundSystem = {
			Begin = tick(),
			AssetID = Arguments.AssetID,
			AudioLength = 0,
			AudioPlayback = Arguments.PlaybackSpeed or 1,
			AudioVolume = Arguments.Volume or 3.5,
			DoesLoop = Arguments.Loop or false,
			StartingPosition = Arguments.StartPosition or 0,
			KeepPositionOnLoop = Arguments.KeepPositionOnLoop or false,
			Adornee = Arguments.Adornee or LocalPlayer.Character:FindFirstChild("HumanoidRootPart"),
			Debounce = false,
			Break = false
		}
		SoundSystem.AudioLength = ABizarreSoundSystem:Perform({Action = "Return", Request = "AudioLength", AssetID = Arguments.AssetID})
		if not SoundSystem.AudioLength or not SoundSystem.Adornee then 
			return 
		end
		function SoundSystem:Perform(SoundArguments)
			if SoundSystem.Debounce == false then
				if SoundArguments.Action == "Create" then
					if game:GetService("Lighting").TS.Value == false then
						ABizarreSoundSystem.AudioRemote:FireServer(
							LocalPlayer.Character:FindFirstChildOfClass("Humanoid"), -- Humanoid
							SoundSystem.Adornee.CFrame, -- CFrame 
							0, -- Damage
							0, -- Velocity Debris
							Vector3.new(), -- Velocity
							9e999, -- Size Increase
							"rbxassetid://"..SoundSystem.AssetID, -- Audio ID
							SoundSystem.AudioPlayback, -- Audio Playback Speed
							SoundSystem.AudioVolume -- Audio Volume
						)
					end
				elseif SoundArguments.Action == "FixTime" then
					game:GetService("RunService").RenderStepped:Wait()
					SoundArguments.Audio.TimePosition = SoundSystem.StartingPosition + ((tick() - SoundSystem.Begin) * SoundSystem.AudioPlayback)
				elseif SoundArguments.Action == "Change" then
					if SoundArguments.Change == "PlaybackSpeed" then
						SoundSystem.AudioPlayback = SoundArguments.Value
					elseif SoundArguments.Change == "Volume" then
						SoundSystem.AudioVolume = SoundArguments.Value
					elseif SoundArguments.Change == "Loop" then
						SoundSystem.DoesLoop = SoundArguments.Value
					elseif SoundArguments.Change == "Adornee" then
						SoundSystem.Adornee = SoundArguments.Value
					elseif SoundArguments.Change == "KeepPositionOnLoop" then
						SoundSystem.KeepPositionOnLoop = SoundArguments.Value
					end
				elseif SoundArguments.Action == "Play" then
					local function PlayAudio(Updates)
						SoundSystem.Begin = (tick() - 0.015)
						for i = 1, SoundArguments.Updates do 
							if SoundSystem.Break == true or SoundSystem == nil or not SoundSystem then
								break
							end
							SoundSystem:Perform({Action = "Create"})
							wait(ABizarreSoundSystem.GlobalExistenceTime)
						end
						if SoundSystem and SoundSystem.DoesLoop == true and SoundSystem.Break ~= true and SoundSystem ~= nil then
							if SoundSystem.KeepPositionOnLoop ~= true then
								SoundSystem.StartingPosition = 0
								SoundArguments.Updates = math.ceil(((SoundSystem.AudioLength / ABizarreSoundSystem.GlobalExistenceTime) / SoundSystem.AudioPlayback))
							end
							PlayAudio(SoundArguments.Updates)
						else
							SoundSystem:Perform({Action = "Break"})
							ABizarreSoundSystem.Sounds[SoundSystem.AssetID] = nil
							SoundSystem = nil
						end
					end
					PlayAudio(SoundArguments.Updates)
				elseif SoundArguments.Action == "Break" then
					SoundSystem.Break = true
					SoundSystem.Debounce = true
				end
			end
		end 
		ABizarreSoundSystem.Sounds["rbxassetid://"..SoundSystem.AssetID] = SoundSystem
		if (SoundSystem.AudioLength - SoundSystem.StartingPosition) < ABizarreSoundSystem.GlobalExistenceTime or Arguments.Short == true then
			SoundSystem:Perform({Action = "Create"})
		elseif (SoundSystem.AudioLength - SoundSystem.StartingPosition) > ABizarreSoundSystem.GlobalExistenceTime and Arguments.Short ~= true then
			Thread(function()
				local UpdateCount = math.ceil(((SoundSystem.AudioLength - SoundSystem.StartingPosition) / ABizarreSoundSystem.GlobalExistenceTime) / SoundSystem.AudioPlayback)
				SoundSystem:Perform({Action = "Play", Updates = UpdateCount})
			end)
		end
	end
end

workspace.Effects.DescendantAdded:Connect(function(Object)
	if Object:IsA("Sound") then
		local System = ABizarreSoundSystem.Sounds[Object.SoundId]
		if System then
			System:Perform({Action = "FixTime", Audio = Object})
		end
	end
end)
function PlayAudioRewrited(Id,Pitch,Volume,Loop,StartPosition,LoopPosition)
ABizarreSoundSystem:Perform({
	Action = "Create",
	AssetID = Id, -- Audio ID
	PlaybackSpeed = Pitch, -- Pitch
	Volume = Volume,
	Loop = Loop, -- Loops the song
	StartPosition = StartPosition, -- Starts the song at this time position, 0 or nil for no change
	KeepPositionOnLoop = LoopPosition -- Starts the song at the given time position on loop, keep false to start over
})
end
function Preload(Id)
ABizarreSoundSystem:Perform({
	Action = "Preload",
	AssetID = Id -- Audio ID
})
end
-- Sound Stopping
function StopSound(Id)
ABizarreSoundSystem:Perform({
	Action = "Destroy",
	AssetID = Id -- Audio ID, set to "All" in order to stop all ongoing sounds
})
end
-- Sound Changing
function ChangeVol(Volume,Id)
ABizarreSoundSystem:Perform({
	Action = "Change",
 
	Change = "Volume",
 
	AssetID = Id, -- Audio ID, set to "All" in order to change all sounds
	Value = Volume -- Changed Value
})
end
function ChangePitch(Pitch,Id)
ABizarreSoundSystem:Perform({
	Action = "Change",
 
	Change = "PlaybackSpeed",
 
	AssetID = Id, -- Audio ID, set to "All" in order to change all sounds
	Value = Pitch -- Changed Value
})
end
