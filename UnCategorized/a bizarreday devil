        while wait(5) do
            if HalfDemon == true then
	chr.Animate.walk.WalkAnim.AnimationId = "rbxassetid://5166114330"
	chr.Animate.fall.FallAnim.AnimationId = "rbxassetid://5166133986" 
	chr.Animate.jump.JumpAnim.AnimationId = "rbxassetid://2763931895" 
	chr.Animate.idle.Animation1.AnimationId = "rbxassetid://5166116703"
     elseif HalfDemon == false then
